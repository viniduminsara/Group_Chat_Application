<AnchorPane prefHeight="696.0" prefWidth="600.0" style="-fx-background-color: #2f3e46;" stylesheets="@../style/style.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ijse.chatApp.controller.ClientFormController">
