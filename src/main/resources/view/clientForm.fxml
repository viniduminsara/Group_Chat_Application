            </ImageView>
            <ImageView fitHeight="30.0" fitWidth="30.0" layoutX="177.0" layoutY="64.0" onMouseClicked="#thumbsUpOnAction" pickOnBounds="true">
               <image>
                  <Image url="@../img/emoji/thumbsup.png" />
               </image>
            </ImageView>
