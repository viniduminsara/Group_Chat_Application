      </ImageView>
      <TextField fx:id="txtMessage" layoutX="21.0" layoutY="632.0" onAction="#txtMessageOnAction" prefHeight="38.0" prefWidth="495.0" promptText="Type here" style="-fx-background-color: #445159; -fx-text-fill: #828b90; -fx-background-radius: 20px;" />
      <JFXButton contentDisplay="GRAPHIC_ONLY" layoutX="523.0" layoutY="632.0" onAction="#btnSendOnAction" prefHeight="38.0" prefWidth="61.0" style="-fx-background-color: #57cc99; -fx-background-radius: 20px;">
